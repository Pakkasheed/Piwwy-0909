local Tab = Window:NewTab("Misc")
local Section = Tab:NewSection("miscellaneous")
Section:NewToggle("Setspawn", "By ! Piwwy#0909", function(state)
    if state then
repeat    
        local args = {
    [1] = "SetSpawnPoint"
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
until false
    else
        print("Toggle Off")
        repeat    
        local args = {
    [1] = "SetSpawnPoint"
}
wait(7)
game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
until true
    end
end)
Section:NewToggle("Auto random fruit", "By ! Piwwy#0909", function(state)
    if state then
repeat 

local args = {
    [1] = "Cousin",
    [2] = "Buy"
}
wait()
game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
until false

        print("Toggle Off")

    end
end)
local Section = Tab:NewSection("Dialogue")
    Section:NewButton("Fruit Shop", "By ! Piwwy#0909", function()
        game:GetService("Players").LocalPlayer.PlayerGui.Main.FruitShop.Visible = true
        end)
    Section:NewButton("Weapon Inventory", "By ! Piwwy#0909", function()
        game:GetService("Players").LocalPlayer.PlayerGui.Main.Inventory.Visible = true
    end)
    Section:NewButton("Fruit Inventory", "By ! Piwwy#0909", function()
        game:GetService("Players").LocalPlayer.PlayerGui.Main.FruitInventory.Visible = true
end)
